{
  "name": "emailjs-addressparser",
  "version": "2.0.0",
  "homepage": "https://github.com/emailjs/emailjs-addressparser",
  "description": "Parse rfc2822 address fields",
  "author": "Andris Reinman <andris@kreata.ee>",
  "keywords": [
    "RFC822",
